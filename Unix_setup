export GREP_COLOR='1;92'
export GPGKEY=769D777E
LS_COLORS='di=92:fi=0:ln=0:pi=0:so=0:bd=0:cd=0:or=0:mi=0:ex=0'
# General UNIX
alias la='ls -lah --color'
alias ls='ls -h --color'
alias grep='grep --color=auto'
